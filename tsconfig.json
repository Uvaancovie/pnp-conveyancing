{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["ES2020", "DOM"],
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "supabase/functions/**",
    "supabase/config.toml",
    "node_modules",
    "dist",
    "build"
  ]
}
